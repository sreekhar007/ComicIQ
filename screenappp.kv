<dc>:
    FloatLayout:
        canvas.before:
            Color:
                rgba:.6,.3,.9,.6
            Rectangle:
                pos:self.pos
                size:self.size
        GridLayout:
            padding:30
            spacing:30
            cols:2
            rows:2
            Button:
                size_hint:.3,.2
                text:"Super Man"
                font_size:25
                background_color:"black"


            Button:
                size_hint:.3,.2
                text:"Batman"
                font_size:50
                background_color:"black"
            Button:
                size_hint:.3,.2
                text:"Joker"
                font_size:50
                background_color:"black"
            Button:
                size_hint:.3,.2
                text:"Flash"
                font_size:50
                background_color:"black"
        Button:
            text:"Back"
            size_hint:.05,.05
            pos_hint:{"center_x":.05,"center_y":.95}
            background_color:"black"
            on_press:
                root.manager.current = "level"
                root.manager.transition.direction = "right"




