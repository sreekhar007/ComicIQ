#home page

<level_button@Button>:
    size_hint:None,None
    width:dp(300)
    height:dp(150)


<home>:
    FloatLayout:
        canvas.before:


            Rectangle:
                pos:self.pos
                size:self.size
                source:"back.png"




        Image:
            source:"ll.png"
            size_hint:None,None
            width:dp(130)
            height:dp(150)
            pos_hint:{"center_x":.5,"center_y":.7}



        Label:
            id:player_name
            on_ref_press:root.changing()



            canvas.before:
                Color:
                    rgb:(1,1,1)
                Rectangle:
                    pos:self.pos
                    size:self.size

            pos_hint:{"left":.07,"top":1}
            color:"black"
            size_hint:None,None
            width:dp(130)
            height:dp(40)
            font_size:'20sp'
            bold:True
        Label:
            canvas.before:
                Color:
                    rgb:(1,1,1)
                Rectangle:
                    pos:self.pos
                    size:self.size
            id:iqlogic

            pos_hint:{"right":1,"top":1}
            color:"black"
            size_hint:None,None
            width:dp(130)
            height:dp(40)
            font_size:'20sp'
            bold:True






        Button:

            size_hint:None,None
            background_normal:""
            width:dp(160)
            height:dp(45)
            background_color:(7/255, 248/255, 242/255)



            pos_hint:{"center_x":.5,"center_y":.43}

            on_press:
                root.manager.current = "level"
                root.button()


                root.manager.transition.direction = "left"
                root.manager.transition.duration = .6
            Image:

                source:"playf.png"



                size_hint:None,None
                width:dp(150)
                height:dp(70)

                center_x:self.parent.center_x
                center_y:self.parent.center_y





        Button:
            text:"Settings"
            size_hint:None,None
            width:dp(160)
            height:dp(45)
            font_size:'20sp'
            background_normal:""

            background_color:(7/255, 248/255, 242/255)
            pos_hint:{"center_x":.5,"center_y":.31}
            Image:

                source:"set.png"



                size_hint:None,None
                height:dp(70)
                width:dp(150)

                center_x:self.parent.center_x
                center_y:self.parent.center_y

<level>:
    FloatLayout:
        canvas.before:

            Rectangle:
                pos:self.pos
                size:self.size
                source:"back.png"


        Label:
            text:"Test Your CIQ"
            font_size:'25sp'

            pos_hint:{"center_x":.5,"center_y":.9}


        level_button:

            pos_hint:{"center_x":.5,"center_y":.6}
            on_press:
                root.manager.current = "marvel"
                root.manager.transition.direction = "down"


            Image:

                source:"marvel.png"

                size_hint:None,None
                height:dp(150)
                width:dp(300)

                center_x:self.parent.center_x
                center_y:self.parent.center_y

        level_button:
            id:comming
            pos_hint:{"center_x":.5,"center_y":.3}
            on_release:root.pop()
            on_press:
                root.commingsoon()
                #root.manager.current = "dc"
                #root.manager.transition.direction = "up"

            Image:
                id:dcimg

                source:"dc.png"



                size_hint:None,None
                height:dp(150)
                width:dp(300)

                center_x:self.parent.center_x
                center_y:self.parent.center_y

        Button:

            size_hint:None,None
            width:dp(20)
            height:dp(20)
            background_down:""
            background_color:9/255, 200/255, 175/255,.4
            background_normal:""

            pos_hint:{"center_x":.08,"center_y":.96}
            on_press:
                root.manager.current = "home"
                root.manager.transition.direction = "right"

            Image:

                source:"bk.png"



                size_hint:None,None
                height:dp(70)
                width:dp(70)

                center_x:self.parent.center_x
                center_y:self.parent.center_y



